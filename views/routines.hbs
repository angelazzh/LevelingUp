<div class="container">
  <header>
    <a href="/" style="text-decoration: none; color: inherit;">
      <img class="logo" src="/images/logo.png" alt="Logo">
      <h1>LEVELING UP</h1>
    </a>
  </header>
  <div class="log">
    <a href="/logout">Logout</a>
  </div>
  <div class = "center">
    <h1>Your Workout Routines</h1>
    <ul>
      {{#each routines}}
        <li>
          <a href="/routines/{{this._id}}">{{this.name}}</a>
        </li>
      {{/each}}
    </ul>
    <a href="/routines/create">Create Routine</a>
    <br>
    <a href="/"><-- Home🏠</a>
  </div>
</div>



